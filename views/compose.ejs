<%- include("partials/header"); -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">

  <div class="form-group">
      <label for="postTitle">Title</label>
      <input type="text" class="form-control" name="postTitle" required>
    </div>

  <div class="form-group">
    <label for="postBody">Post</label>
    <textarea class="form-control"  name="postBody" rows="6" required></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
<a href="/" class="btn btn-success" name="button">Go home</a>
</form>



<%- include("partials/footer"); -%>
